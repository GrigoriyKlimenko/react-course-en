{
  "name": "react-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --e2e",
    "cypress:report": "npx nyc report && npx nyc report --reporter=text-summary",
    "cypress:e2e": "npx start-server-and-test dev http-get://localhost:5173 cypress:run",
    "test:cypress": "npm run cypress:e2e && npm run cypress:report"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "@types/react-redux": "^7.1.25",
    "cypress": "^12.11.0",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "ts-node": "^10.9.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-tsconfig-paths": "^4.2.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-istanbul": "^0.29.3",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.29.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
